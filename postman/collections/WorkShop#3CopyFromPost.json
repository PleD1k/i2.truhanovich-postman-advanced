						"exec": [
							"let jsonData = pm.response.json();\r",
							"\r",
							"console.log(`Pokemon Type: ${jsonData.types[0].type.name}`);\r",
							"\r",
							"pm.sendRequest(\"https://pokeapi.co/api/v2/gender/male\", function (err, response) {\r",
							"    if (err) {\r",
							"        console.error(err);\r",
							"        return;\r",
							"    }\r",
							"    \r",
							"    let genderData = response.json();\r",
							"\r",
							"    let pikachuUrl = pm.collectionVariables.get(\"pikachuUrl\");\r",
							"    \r",
							"    pm.test(\"Pikachu is male\", function () {\r",
							"        pm.expect(genderData.pokemon_species_details.some(function (species) {\r",
							"            return species.pokemon_species.name === \"pikachu\" && species.pokemon_species.url === \"https://pokeapi.co/api/v2/pokemon-species/25/\";\r",
							"        })).to.be.true;\r",
							"    });\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
