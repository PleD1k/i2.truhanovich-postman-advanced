						"exec": [
							"let jsonData = pm.response.json();\r",
							"\r",
							"if (jsonData.results) {\r",
							"    pm.collectionVariables.set(\"pokemonResults\", JSON.stringify(jsonData.results));\r",
							"}\r",
							"\r",
							"let pikachuUrl = null;\r",
							"for (let i = 0; i < jsonData.results.length; i++) {\r",
							"    if (jsonData.results[i].name === \"pikachu\") {\r",
							"        pikachuUrl = jsonData.results[i].url;\r",
							"        break;\r",
							"    }\r",
							"}\r",
							"\r",
							"if (pikachuUrl) {\r",
							"    pm.collectionVariables.set(\"pikachuUrl\", pikachuUrl);\r",
							"}\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
